{
  "name": "bookclick",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
    "homepage": "http://danieldrojas.github.io/bookclick",
    "proxy": "http://localhost:8000/",
  "scripts": {
    "start": "kill -9 $(lsof -ti:3000,8000) && if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node src/server.js",
    "start:dev": "concurrently --kill-others \"npx nodemon --ignore 'client/*' --exec npx babel-node src/server.js \" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "seed": "node scripts/seedDB.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Daniel Rojas",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "if-env": "^1.0.4",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.8",
    "nodemon": "^2.0.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
